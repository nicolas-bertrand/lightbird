<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <!-- If true the files put in the filesPath of the server are automatically
       added to the database. -->
  <filesPathWatcher>true</filesPathWatcher>
  <!-- The time in minutes to check and remove the files that could not be
       removed before from the filesPath. -->
  <removeFileTimer>60</removeFileTimer>
</configuration>
