<?xml version='1.0' encoding='UTF-8'?>
<configuration>
  <name>Tarsiger cyanurus</name>
  <pluginsPath>plugins</pluginsPath>
  <QtPluginsPath>QtPlugins</QtPluginsPath>
  <filesPath>files</filesPath>
  <filesExtensionsPath></filesExtensionsPath>
  <temporaryPath>tmp</temporaryPath>
  <cleanTemporaryPath>true</cleanTemporaryPath>
  <languagesPath>languages</languagesPath>
  <language>fr</language>
  <threadsNumber>10</threadsNumber>
  <hashSizeLimit>-1</hashSizeLimit>
  <ports>
    <port protocol="HTTP" transport="TCP" maxClients="100">80</port>
    <port protocol="HTTP" transport="TCP" maxClients="100">4242</port>
  </ports>
  <plugins>
    <plugin>Files</plugin>
    <plugin>Http/Client</plugin>
    <plugin>Http/Parser</plugin>
    <plugin>Json</plugin>
    <plugin>Log/File</plugin>
    <!--plugin>Test</plugin-->
  </plugins>
  <log>
    <level>Trace</level>
    <display>true</display>
    <file>server.log</file>
    <path>logs</path>
    <maxNbOfFile>10</maxNbOfFile>
    <maxSize>1M</maxSize>
    <expires>30</expires>
  </log>
  <database>
    <name/>
    <file>database.sqlite</file>
    <path>databases</path>
    <resource>:database</resource>
    <type>QSQLITE</type>
    <password/>
    <user/>
    <host/>
    <port/>
    <options/>
    <pragmas>
      <pragma>PRAGMA recursive_triggers=true</pragma>
      <pragma>PRAGMA synchronous=false</pragma>
    </pragmas>
  </database>
  <permissions>
    <activate>false</activate>
    <default>false</default>
    <inheritance>true</inheritance>
    <ownerInheritance>true</ownerInheritance>
    <groupInheritance>true</groupInheritance>
  </permissions>
  <preview>
    <defaultHeight>300</defaultHeight>
    <defaultFormat>jpeg</defaultFormat>
    <cacheEnabled>true</cacheEnabled>
    <cachePath>cache</cachePath>
    <cacheSizeLimit>100</cacheSizeLimit>
  </preview>
  <configurations>
    <plugin id="Files"/>
    <plugin id="Ftp"/>
    <plugin id="Http/Client"/>
    <plugin id="Http/Parser"/>
    <plugin id="Json"/>
    <plugin id="Log/File"/>
    <plugin id="Test"/>
  </configurations>
</configuration>
